{
  "compilerOptions": {
    "baseUrl": "./src",
    "checkJs": false,
    "jsx": "react-jsx",
    "types": [],
    "typeRoots": [
      "./node_modules/@types"
    ],
    "allowJs": true,
    "skipLibCheck": true
  },
  "typeAcquisition": {
    "enable": false
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "build"
  ]
}
